{
  "name": "helia-service-worker-gateway",
  "version": "1.12.1",
  "private": true,
  "type": "module",
  "description": "An IPFS Gateway that uses @helia/verified-fetch in a Service Worker to fetch content",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "reset": "npm run clean -- **/node_modules **/package-lock.json",
    "clean": "aegir clean dist dist-tsc test-e2e/fixtures/data/gateway-conformance-fixtures",
    "dep-check": "aegir dep-check",
    "lint": "aegir lint",
    "lint:fix": "aegir lint --fix",
    "build": "run-s build:tsc build:esbuild",
    "build:tsc": "tsc",
    "build:esbuild": "node build.js",
    "prebuild:binary": "npm run build",
    "build:binary": "go build -o service-worker-gateway main.go",
    "predev": "run-s build:tsc",
    "dev": "node dist-tsc/serve.js",
    "prestart": "run-s build:binary",
    "start": "./service-worker-gateway",
    "start:frontend": "node build.js --watch --serve",
    "start:cloudflare": "npm run build && npx wrangler pages dev dist",
    "prestart:hosted": "run-s build:tsc",
    "test": "run-s test:iso 'test:node -- -b false'",
    "pretest:all": "run-s build",
    "test:all": "cross-env SHOULD_BUILD=false run-s 'test:iso -- -b false' 'test:node -- -b false' test:browsers",
    "test:iso": "aegir test -f dist-tsc/test/**/*.spec.js",
    "test:browsers": "playwright test -c playwright.config.js --project chromium firefox no-service-worker",
    "test:chrome": "playwright test -c playwright.config.js --project chromium",
    "test:no-sw": "playwright test -c playwright.config.js --project no-service-worker",
    "test:firefox": "playwright test -c playwright.config.js --project firefox no-service-worker",
    "test:deployed": "playwright test -c playwright.config.js --project deployed",
    "test:local-binary": "concurrently -k --success first -n \"binary,tests\" \"npm run start\" \"cross-env SHOULD_BUILD=false BASE_URL='http://localhost:3000' playwright test -c playwright.config.js --project deployed no-service-worker\"",
    "test:inbrowser-dev": "cross-env BASE_URL='https://inbrowser.dev' playwright test -c playwright.config.js --project deployed",
    "test:inbrowser-prod": "cross-env BASE_URL='https://inbrowser.link' playwright test -c playwright.config.js --project deployed",
    "test:node": "aegir test -t node -f dist-tsc/test/node.js --cov",
    "postinstall": "patch-package"
  },
  "browser": "./dist/src/index.js",
  "browserslist": [
    "last 1 Chrome version"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^16.0.3",
    "@chainsafe/libp2p-yamux": "^7.0.1",
    "@helia/block-brokers": "^4.1.0",
    "@helia/delegated-routing-v1-http-api-client": "^4.2.2",
    "@helia/http": "^2.0.5",
    "@helia/interface": "^5.2.1",
    "@helia/routers": "^3.0.1",
    "@helia/verified-fetch": "^2.6.8",
    "@libp2p/crypto": "^5.1.0",
    "@libp2p/dcutr": "^2.0.28",
    "@libp2p/identify": "^3.0.28",
    "@libp2p/keychain": "^5.2.0",
    "@libp2p/logger": "^5.1.14",
    "@libp2p/ping": "^2.0.28",
    "@libp2p/websockets": "^9.2.9",
    "@libp2p/webtransport": "^5.0.38",
    "@multiformats/dns": "^1.0.6",
    "@noble/hashes": "^1.5.0",
    "execa": "^9.5.2",
    "helia": "^5.3.0",
    "ipfs-css": "^1.4.0",
    "ipfsd-ctl": "^15.0.2",
    "libp2p": "^2.8.3",
    "multiformats": "^13.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tachyons": "^4.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@playwright/test": "^1.48.2",
    "@types/react": "^19.0.1",
    "aegir": "^45.1.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.2",
    "glob": "^11.0.1",
    "http-server": "^14.1.1",
    "it-all": "^3.0.7",
    "kubo": "^0.34.1",
    "kubo-rpc-client": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "playwright": "^1.45.3",
    "rimraf": "^6.0.1",
    "wait-on": "^8.0.3"
  },
  "sideEffects": [
    "*.css"
  ]
}
